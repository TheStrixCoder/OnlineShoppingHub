<?xml version="1.0" encoding="utf-8"?>


<RelativeLayout android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/imageView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignTop="@+id/imageView1"
            android:layout_gravity="center"
            android:layout_marginStart="21dp"
            android:paddingTop="@dimen/nav_header_vertical_spacing"
            android:src="@drawable/bidyadhar"
            app:civ_border_color="#FF000000"
            app:civ_border_width="1dp"
            app:srcCompat="@android:drawable/sym_def_app_icon" />

        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/imageView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentTop="true"
            android:layout_gravity="center"
            android:layout_marginEnd="34dp"
            android:layout_marginTop="21dp"
            android:paddingTop="@dimen/nav_header_vertical_spacing"
            android:src="@drawable/soham"
            app:civ_border_color="#FF000000"
            app:civ_border_width="1dp"
            app:srcCompat="@android:drawable/sym_def_app_icon" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_below="@+id/textView9"
        android:layout_centerHorizontal="true"
        android:layout_gravity="center"
        android:layout_marginTop="14dp"
        android:paddingTop="@dimen/nav_header_vertical_spacing"
        android:src="@drawable/tilak"
        app:civ_border_color="#FF000000"
        app:civ_border_width="1dp"
        app:srcCompat="@android:drawable/sym_def_app_icon" />

        <TextView
            android:id="@+id/bidyadharName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/imageView"
            android:layout_below="@+id/imageView"
            android:layout_marginTop="15dp"
            android:text="Bidyadhar Mohanty"
            android:textColor="@color/md_black_1000"
            android:textSize="15dp" />

        <TextView
            android:id="@+id/sohamName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/bidyadharName"
            android:layout_alignBottom="@+id/bidyadharName"
            android:layout_alignStart="@+id/imageView1"
            android:layout_marginStart="21dp"
            android:text="Soham Patil"
            android:textColor="@color/md_black_1000"
            android:textSize="15dp"
            />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/bidyadharName"
        android:layout_below="@+id/bidyadharName"
        android:text="B.Tech CSE(2nd Year)"
        android:textColor="@color/md_black_1000" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/imageView1"
        android:layout_below="@+id/sohamName"
        android:text="B.Tech CSE(2nd Year)"
        android:textColor="@color/md_black_1000" />

    <TextView
        android:id="@+id/tilakName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageView2"
        android:layout_centerHorizontal="true"
        android:text="Tilak N. Shenoy"
        android:textColor="@color/md_black_1000"
        android:textSize="15dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tilakName"
        android:layout_centerHorizontal="true"
        android:text="B.Tech IT(2nd Year)"
        android:textColor="@color/md_black_1000" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/imageView"
            android:layout_below="@+id/textView6"
            android:text="(Front-End, Back-End)"
            android:textColor="@color/md_black_1000"
            android:textSize="12dp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView9"
        android:layout_alignBottom="@+id/textView9"
        android:layout_alignStart="@+id/sohamName"
        android:text="(Front-End)"
        android:textColor="@color/md_black_1000"
        android:textSize="12dp" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView8"
        android:layout_toStartOf="@+id/sohamName"
        android:text="(Front-End)"
        android:textColor="@color/md_black_1000"
        android:textSize="12dp" />


</RelativeLayout>
